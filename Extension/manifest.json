{
    "manifest_version": 2,
    "name": "EEnstant - Extenstion",
    "version": "0.1",
  
    "description": "A simple extension that enables discord rich presence for yt music",
  
    "icons": {
      "48": "icons/logo-48.png",
      "96": "icons/logo-96.png"
    },
    
    "permissions":[
      "activeTab",
      "*://localhost/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://music.youtube.com/*"],
        "js": ["eenstant.js"]
      }
    ]
  }
  